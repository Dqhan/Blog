    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "attach Program",
            "port": 9222
        },
        {
            "type": "node",
            "request": "attach",
            "name": "attach server",
            "port": 9222,
            "protocol":"inspector",
            "cwd": "${workspaceFolder}/server\\server.js",
            "restart": true
        },
        {
            "type": "node",
            "request": "attach",
            "name": "attach api",
            "port": 9222,
            "protocol":"inspector",
            "cwd":"${workspaceFolder}/server\\api\\api.js",
            "restart": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "launch server",
            "program": "${workspaceFolder}/server/server.js"
        }
    ]
}